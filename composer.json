{
    "name": "roromix/spreadsheetbundle",
    "description": "This is a Symfony 3.2/4/5 Bundle helps you to read and write Spreadsheet files (including pdf, xls, xlsx, odt, csv), thanks to the PHPSpreadsheet library",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Romain Harache",
            "email": "romain.harache@gmail.com"
        }
    ],
    "keywords": ["excel", "phpexcel", "spreadsheet", "symfony", "bundle"],
    "homepage": "https://github.com/roromix/SpreadsheetBundle",
    "license": "MIT",
    "require": {
        "symfony/framework-bundle": "~3.2|~4.0|~5.0",
        "phpoffice/phpspreadsheet": "~1.0"
    },
    "require-dev": {
        "symfony/symfony": "~3.2|~4.0|~5.0",
        "symfony/phpunit-bridge": "~3.2|~4.0|~5.0",
        "sensio/framework-extra-bundle": "~3.0|~4.0|~5.0"
    },
    "autoload": {
        "psr-4": { "Roromix\\Bundle\\SpreadsheetBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Roromix\\Bundle\\SpreadsheetBundle\\Tests\\": "tests/" }
    }
}
