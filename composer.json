{
    "name": "roromix/spreadsheetbundle",
    "description": "This is a Symfony 3.2 to 7 Bundle helps you to read and write Spreadsheet files (including pdf, xls, xlsx, odt, csv), thanks to the PHPSpreadsheet library",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Romain Harache",
            "email": "romain.harache@gmail.com"
        }
    ],
    "keywords": ["excel", "phpexcel", "spreadsheet", "symfony", "bundle"],
    "homepage": "https://github.com/roromix/SpreadsheetBundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "~4.0|~5.0|~6.0|~7.0",
        "phpoffice/phpspreadsheet": "~1.0"
    },
    "autoload": {
        "psr-4": { "Roromix\\Bundle\\SpreadsheetBundle\\": "src/" }
    }
}
